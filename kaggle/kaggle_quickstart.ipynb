0) Banner & helpers

import os, sys, json, time, shutil, subprocess, textwrap, traceback
from pathlib import Path

def banner(title:str):
    pad = "â•"* max(0, 60 - len(title) - 2)
    print(f"\n\033[1;36mâ•”â• {title} {pad}\033[0m")

def sec(s: float)->str:
    return f"{s:.2f}s"

def run_cmd(cmd:list):
    try:
        print("â†’", " ".join(map(str, cmd)))
        t0 = time.time()
        out = subprocess.run(cmd, stdout=subprocess.PIPE, stderr=subprocess.PIPE, text=True)
        dt = time.time() - t0
        print(f"âœ“ finished in {sec(dt)}")
        if out.stdout: print(out.stdout)
        if out.returncode != 0:
            print("\n\033[1;31mâœ— command failed\033[0m")
            if out.stderr: print(out.stderr)
            out.check_returncode()
        return dt
    except subprocess.CalledProcessError as e:
        print("\n\033[1;31mâœ— command raised CalledProcessError\033[0m")
        print(e.stderr or "")
        raise


